{
  "name": "cargo-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --debug --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "command-nest": "ts-node -r tsconfig-paths/register ./src/console.ts",
    "prisma:gen": "rm -rf ./src/prisma-client && rm -rf ./src/types && prisma generate --schema ./prisma/schema.prisma",
    "prisma:ci:gen": "yarn prisma:gen --max-processes '50%'",
    "prisma:migrate": "POSTGRES_URL=postgresql://postgres:postgres@localhost:5432/cargo prisma migrate dev",
    "prisma:migrate:local": "POSTGRES_URL=postgresql://postgres:postgres@localhost:5432/cargo prisma migrate dev",
    "add:migration": "POSTGRES_URL=postgresql://postgres:postgres@localhost:5432/cargo prisma migrate dev --name add_package_relations_to_orders_tables",
    "cli": "ts-node -r tsconfig-paths/register src/cli.ts",
    "db:seed": "ts-node -r tsconfig-paths/register src/cli.ts db:seed"
  },
  "engines": {
    "node": "18.20.3"
  },
  "dependencies": {
    "@atlas/nestjs-prisma-gen": "^0.3.10",
    "@aws-sdk/client-s3": "^3.772.0",
    "@graphql-tools/utils": "^10.5.5",
    "@nestjs/apollo": "^12.2.1",
    "@nestjs/axios": "^3.1.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/graphql": "^12.2.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^8.0.5",
    "@prisma/client": "^6.4.1",
    "@prisma/extension-optimize": "^1.1.8",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.12",
    "@types/uuid": "^10.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "eslint-plugin-node": "^11.1.0",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "globals": "^15.12.0",
    "graphql": "^16.6.0",
    "graphql-query-complexity": "^0.12.0",
    "graphql-scalars": "^1.24.2",
    "graphql-upload": "^17.0.0",
    "joi": "^17.13.3",
    "multer": "1.4.5-lts.2",
    "nest-commander": "^3.16.0",
    "nestjs-s3": "^2.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "prisma": "^6.4.1",
    "prisma-kysely": "^1.8.0",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^5.0.10",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "tmp": "^0.2.3",
    "ts-morph": "^16.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.734.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/graphql-upload": "^17.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
